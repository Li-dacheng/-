}
.jifenitem{
   font-size: 40rpx;
   font-weight: 500;
   width:100rpx;
   display:flex;
   justify-content: center;
}
.jifenitemlong{
   font-size: 40rpx;
   font-weight: 500;
   width:160rpx;
   display:flex;
   justify-content: center;
}