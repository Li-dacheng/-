}
.jifenTitle{
   display:flex;
   justify-content: center;
   width:100%;
   margin:10rpx 5rpx;
   background-color: #DC143C;
   color: #FFFFFF;
   font-size: 40rpx;
}
.jifenrow{
   display:flex;
   width:100%;
